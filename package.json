{
  "name": "agent-twitter-client",
  "description": "A twitter client for agents",
  "keywords": [
    "x",
    "twitter",
    "scraper",
    "crawler"
  ],
  "version": "0.0.5",
  "main": "dist/default/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "node": {
      "import": "./dist/node/esm/index.mjs",
      "require": "./dist/node/cjs/index.cjs"
    },
    "default": {
      "import": "./dist/default/esm/index.mjs",
      "require": "./dist/default/cjs/index.js"
    }
  },
  "author": "Ruby Research",
  "license": "MIT",
  "scripts": {
    "dev": "npm run build:lib && cd client && npm run dev",
    "build": "rimraf dist && rollup -c && cd client && npm run build",
    "build:lib": "rollup -c",
    "preview": "cd client && npm run preview"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.32.20",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "headers-polyfill": "^3.1.2",
    "json-stable-stringify": "^1.0.2",
    "otpauth": "^9.2.2",
    "set-cookie-parser": "^2.6.0",
    "tough-cookie": "^4.1.2",
    "tslib": "^2.5.2"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.0",
    "@types/json-stable-stringify": "^1.0.34",
    "@types/set-cookie-parser": "^2.4.2",
    "@types/tough-cookie": "^4.0.2",
    "esbuild": "^0.21.5",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.0.4"
  }
}
